{
  "manifest_version": 3,
  "name": "Job Application Form Scanner",
  "version": "1.0.0",
  "description": "Scan and extract form fields from job application pages",
  "permissions": [
    "activeTab",
    "scripting",
    "storage"
  ],
  "host_permissions": [
    "http://localhost:5050/*"
  ],
  "action": {
    "default_popup": "popup.html"
  }
}
